{
  "name": "@akadenia/api",
  "version": "1.1.0",
  "description": "Akadenia library: API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "axios-api",
    "akadenia-api",
    "api",
    "akadenia"
  ],
  "engines": {
    "node": ">=20.x"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "test": "jest",
    "format": "prettier --write \"./**/*.{ts,tsx,js,jsx,json,yml}\"",
    "lint": "prettier --check \"./**/*.{ts,tsx,js,jsx,json,yml}\"",
    "prepare": "npm run build",
    "version": "git add -A src",
    "updateLink": "npm run build && npm rm @akadenia/api -g && npm link"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akadenia/AkadeniaApi.git"
  },
  "author": "Akadenia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akadenia/AkadeniaApi/issues"
  },
  "homepage": "https://github.com/akadenia/AkadeniaApi#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "axios": "^1.7.4",
    "axios-retry": "~3.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@jest/globals": "^29.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "prettier": "^3.2.5",
    "semantic-release": "^24.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.3"
  }
}
